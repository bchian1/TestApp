<div>
    <button class="btn btn-success" onclick="location.href = '/Home/Create'">Create</button>
</div>
@if (Model?.formRecords != null)
{
<div class="container">
    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">First</th>
                <th scope="col">Last</th>
                <th scope="col">Username</th>
                <th scope="col">Password</th>
                <th scope="col">From</th>
                <th scope="col">To</th>
                <th scope="col">Email</th>
                <th scope="col">Birthday</th>
                <th scope="col">Favorite Drink</th>
                <th scope="col">Favorite Number</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.formRecords)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                    <td>@item.UserName</td>
                    <td>@item.Password</td>
                    <td>@item.FromDate</td>
                    <td>@item.ToDate</td>
                    <td>@item.Email</td>
                    <td>@item.Birthday</td>
                    <td>@item.FavoriteDrink</td>
                    <td>@item.FavoriteNumber</td>
                </tr>
            }
        </tbody>
    </table>
</div>
}
